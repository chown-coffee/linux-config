alias clear='clear && $HOME/.scripts/shell/coffeefetch'
alias c='clear'
alias reset='reset && $HOME/.scripts/shell/coffeefetch'
alias cp='$HOME/.scripts/shell/cp.sh'
alias mv='$HOME/.scripts/shell/mv.sh'
alias fucking='sudo'
alias fckng='sudo'
alias please='sudo'
alias pls='sudo'
alias ls='ls --color'
alias lse='$HOME/.scripts/shell/lse.sh'
alias lseh='$HOME/.scripts/shell/lse.sh -a'
alias lsel='$HOME/.scripts/shell/lse.sh -l'
alias lsehl='$HOME/.scripts/shell/lse.sh -al'
alias lselh='$HOME/.scripts/shell/lse.sh -la'
alias clse='clear ; echo "" ; lse'
alias clseh='clear ; echo "" ; lseh'
alias clsel='clear ; echo "" ; lsel'
alias clsehl='clear ; echo "" ; lsehl'
alias clselh='clear ; echo "" ; lselh'
alias p-wd='$HOME/.scripts/shell/pwd.sh'
alias spt='launchspt'
alias setw='$HOME/.scripts/dwm/setwallpaper.sh'
alias grep='grep --color=auto'
alias drop_cache_soft='sudo $HOME/.scripts/cache/drop_cache_soft'
alias drop_cache_hard='sudo $HOME/.scripts/cache/drop_cache_hard'
alias drop_cache_full='sudo $HOME/.scripts/cache/drop_cache_full'
alias drop_swap='sudo $HOME/.scripts/swap/drop_swap.sh'
