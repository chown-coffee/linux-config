alias lsel='$HOME/.scripts/shell/pwd.sh ; exa --icons --group-directories-first -l'
alias lsehl='$HOME/.scripts/shell/pwd.sh ; exa --icons --group-directories-first -al'
alias lselh='$HOME/.scripts/shell/pwd.sh ; exa --icons --group-directories-first -la'
